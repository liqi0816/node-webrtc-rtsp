{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "启动程序",
            "program": "${workspaceFolder}/build/server/server.js",
            "skipFiles": [
                "node_modules/**/*.js",
                "lib/**/*.js",
                "runtime/**/*.js",
                "async_hooks.js",
                "inspector_async_hook.js",
                "<node_internals>/**/*.js"
            ],
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}"
        }
    ]
}